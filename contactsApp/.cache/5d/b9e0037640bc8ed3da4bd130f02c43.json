e":"src/components/Counter.js","original":{"line":56,"column":15}}],"sources":{"src/components/Counter.js":"import React, { Component } from 'react';\r\n\r\nclass Counter extends Component {\r\n  constructor(props) {\r\n    super(props);\r\n    this.state = {\r\n      count: this.props.count,\r\n      counting: false,\r\n    };\r\n    this.handleIncrement = this.handleIncrement.bind(this);\r\n    this.handleDecrement = this.handleDecrement.bind(this);\r\n    this.handleReset = this.handleReset.bind(this);\r\n    // this.makeCounter();\r\n  }\r\n  handleIncrement() {\r\n    this.setState({\r\n      count: this.state.count + 1,\r\n      counting: true,\r\n    });\r\n  }\r\n  handleDecrement() {\r\n    this.setState((prevState) => ({\r\n      count: prevState.count > 0 ? prevState.count - 1 : prevState.count,\r\n    }));\r\n  }\r\n  handleReset() {\r\n    this.setState({\r\n      count: 0,\r\n      counting: false,\r\n    });\r\n  }\r\n  // makeCounter() {\r\n  //   setInterval(() => {\r\n  //     this.setState({\r\n  //       count: this.state.count < 10 ? this.state.count + 1 : 0,\r\n  //     });\r\n  //   }, 1000);\r\n  // }\r\n  render() {\r\n    return (\r\n      <div>\r\n        <p>Count: {this.state.count}</p>\r\n        <button onClick={this.handleIncrement}>Increment</button>\r\n        <button onClick={this.handleDecrement}>Decrement</button>\r\n        <button onClick={this.handleReset}>Reset</button>\r\n        <h3>\r\n          {this.state.counting\r\n            ? 'The number is counting'\r\n            : 'The number is not counting'}\r\n        </h3>\r\n      </div>\r\n    );\r\n  }\r\n}\r\n\r\nexport default Counter;\r\n"},"lineCount":null}},"error":null,"hash":"9de473e059c44f1f4463ac2c8e177633","cacheData":{"env":{}}}