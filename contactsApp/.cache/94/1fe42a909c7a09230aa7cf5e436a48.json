r extends Component {\r\n  state = {\r\n    playerOneScore: 0,\r\n    playerTwoScore: 0,\r\n    winningScore: 5,\r\n    gameOver: false,\r\n  };\r\n  handlePlayerOne = () => {\r\n    this.setState({\r\n      playerOneScore: Math.floor(Math.random() * 5) + 1,\r\n      gameOver:\r\n        this.state.playerOneScore === this.state.winningScore ||\r\n        this.state.playerTwoScore === this.state.winningScore\r\n          ? true\r\n          : false,\r\n    });\r\n  };\r\n  handlePlayerTwo = () => {\r\n    this.setState({\r\n      playerTwoScore: Math.floor(Math.random() * 5) + 1,\r\n      gameOver:\r\n        this.state.playerTwoScore === this.state.winningScore ||\r\n        this.state.playerOneScore === this.state.winningScore\r\n          ? true\r\n          : false,\r\n    });\r\n  };\r\n\r\n  reset = () => {\r\n    this.setState({\r\n      playerOneScore: 0,\r\n      playerTwoScore: 0,\r\n      gameOver: false,\r\n    });\r\n  };\r\n  render() {\r\n    return (\r\n      <div className='container'>\r\n        <h2>Player VS Player</h2>\r\n        <div className='playerSection'>\r\n          <PlayerOne\r\n            handlePlyOne={this.handlePlayerOne}\r\n            playersData={this.state}\r\n          />\r\n        </div>\r\n        <div className='playerSection'>\r\n          <PlayerTwo\r\n            handlePlyTwo={this.handlePlayerTwo}\r\n            playersData={this.state}\r\n          />\r\n        </div>\r\n        <p className='winning'>Winning Score {this.state.winningScore}</p>\r\n        <button onClick={this.reset}>Reset</button>\r\n      </div>\r\n    );\r\n  }\r\n}\r\n\r\nexport default Player;\r\n"},"lineCount":null}},"error":null,"hash":"ac3b31eb67b45beb335a0c2a0039e6b5","cacheData":{"env":{}}}